{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/fintech/src",
  "monorepo": true,
  "root": "apps/fintech",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/fintech/tsconfig.app.json"
  },
  "projects": {
    "fintech": {
      "type": "application",
      "root": "apps/fintech",
      "entryFile": "main",
      "sourceRoot": "apps/fintech/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fintech/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    }
  }
}